{
  "name": "node-starter",
  "version": "0.0.0",
  "license": "MIT",
  "main_path": "src/main.ts",
  "scripts": {
    "prod": "ts-node --project src ${npm_package_main_path}",
    "dev": "ts-node --project src ${npm_package_main_path} --NODE_ENV development",
    "debug": "ts-node --inspect=3334 --project src ${npm_package_main_path} --NODE_ENV development",
    "debug-brk": "ts-node  --inspect-brk=3334 --project src ${npm_package_main_path} --NODE_ENV development",
    "test": "ng test",
    "lint": "tslint \"src/**/*.ts\"",
    "tunnel-admin": "ngrok http -subdomain=pixiphi -region=eu 4200",
    "tunnel-frontend": "ngrok http -subdomain=pixiphi -region=eu 3333",
    "tunnel-websocket": "ngrok http --subdomain=pixiphi-ws -region=eu 9999",
    "db-script-clean": "rm -rf migrations/*.js",
    "db-script-transpile": "tsc migrations/*.ts",
    "db-script-begin": "npm run db-script-clean && npm run db-script-transpile",
    "db-script-migrate": "npm run db-script-begin && node_modules/.bin/sequelize db:migrate && npm run db-script-finish",
    "db-script-migrate-undo": "npm run db-script-begin && node_modules/.bin/sequelize db:migrate:undo && npm run db-script-finish",
    "db-script-finish": "npm run db-script-clean"
  },
  "private": true,
  "dependencies": {
    "@types/request": "^2.47.0",
    "body-parser": "^1.18.2",
    "core-js": "^2.5.3",
    "express": "^4.16.2",
    "express-serve-static-core": "^0.1.1",
    "helmet": "^3.11.0",
    "injection-js": "^2.2.1",
    "morgan": "^1.9.0",
    "node-gyp": "^3.6.2",
    "performance-now": "^2.1.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^5.5.6",
    "ts-helpers": "^1.1.2",
    "winston": "^2.4.0",
    "ws": "^4.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.8",
    "@types/helmet": "0.0.37",
    "@types/jasmine": "2.8.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.5",
    "@types/winston": "^2.3.8",
    "@types/ws": "^4.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.1"
  }
}

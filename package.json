{
  "name": "starter",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "main_path": "src/main.ts",
  "scripts": {
    "ng": "ng",
    "preinstall": "npm install -g @angular/cli && echo 'preinstall executed'",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start-project": "ts-node --project src ${npm_package_main_path}",
    "start-debug-project": "ts-node --project --debug-brk src ${npm_package_main_path}",
    "workflow-prod": "npm run start-project",
    "workflow-dev": "npm run start-project --NODE_ENV development",
    "workflow-debug": "npm run start-debug-project --NODE_ENV development",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@types/body-parser": "0.0.33",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "@types/winston": "^2.2.0",
    "@types/engine.io": "https://github.com/lu4/types-engine.io.git",
    "@types/helmet": "0.0.33",
    "@types/morgan": "^1.7.32",
    "body-parser": "^1.16.0",
    "core-js": "^2.4.1",
    "engine.io": "^2.0.1",
    "express": "^4.14.1",
    "express-serve-static-core": "^0.1.1",
    "helmet": "^3.4.0",
    "injection-js": "^1.0.3",
    "jwt-simple": "^0.5.1",
    "morgan": "^1.7.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "angular-cli": "^1.0.0-beta.26",
    "codelyzer": "^2.0.0-beta.4",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "tslint": "^4.0.2",
    "typescript": "~2.0.3"
  }
}
